funcao principal()
  variavel ve 
 
  ve = {8,2,7,3,6,4,5,0,1}
  
  bolha(ve)
  
  imprimaVetor(ve)
fim

funcao bolha(v)
  variavel i, j, tam, t
  tam = tamanho(v) - 1
  
  para t = 1 ate tam
    para i = 1 ate tam
        j = i-1
        se v[j] > v[i] entao
          aux = v[i]
          v[i] = v[j]
          v[j] = aux         
        fim
    fim
  fim
fim