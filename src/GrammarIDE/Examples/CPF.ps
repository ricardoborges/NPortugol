função principal()
   variável vet, tam, resultado
 
   vet = vetorizar("11111111111")

   resultado = valida(vet)

imprima(resultado)

fim

função valida(cpf)

	se tamanho(cpf) < 11 retorne falso fim
	se validaDigito(cpf, cpf[9], 9) == falso retorne falso fim
	se validaDigito(cpf, cpf[10], 10) == falso retorne falso fim

	retorne verdadeiro
fim



função validaDigito(cpf, dig, tam)
	variável soma, seq, digcalc, i, num, r

        soma = 0
        seq = tam + 1

	para i = 0 até tam
           num = cpf[i]
           soma = soma + num * seq
           seq = seq - 1
	fim

        r = soma % 11

        se r < 2 
            digcalc = 0 
        senão
            digcalc = 11 - r
        fim

        se digcalc == dig retorne verdadeiro fim


        retorne falso
fim
