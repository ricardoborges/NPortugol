<html>
    <head></head>
    <body>
    <ul>
        <% variavel i %>

        <% para i = 0 ate 10 %>

        <li>
            <%escreva("Fatorial de ", i, " = ") %> <input type='text' value='<% escreva(fat(i))%>' />
        </li>

        <% fim %>
    </ul>
    </body>
</html>

<%
funcao fat(x)
  se x == 0 retorne 1 fim
  
  retorne fat(x - 1) * x
fim
%>